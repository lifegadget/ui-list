{{#if hasBlock}}
  {{yield this}}
{{else}}
{{#each arrangedContent as |item index|}}
    {{component itemType
        list=this
        data=item
        map=mappedProperties
        tabindex=tabindex
        _mappedFrom=_mappedFrom
        defaultValues=defaultValues
        _itemSetProperties=_itemSetProperties
        size=size
        style=style
        mood=mood
        squeezed=squeezed
        index=index
    }}
{{/each}}
{{/if}}
